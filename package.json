{
    "name": "ngx-feature-flag-router",
    "version": "13.0.1",
    "description": "Extends RouteModule functionality to allow for conditionally lazy-loading child routes based on feature flag",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/m-thompson-code/ngx-feature-flag-router.git"
    },
    "bugs": {
        "url": "https://github.com/m-thompson-code/ngx-feature-flag-router/issues"
    },
    "homepage": "https://m-thompson-code.github.io/ngx-feature-flag-router/",
    "author": "Mark Thompson",
    "keywords": [
        "nx",
        "ng",
        "angular",
        "router",
        "RouterModule",
        "lazy loading",
        "conditional",
        "feature flag",
        "loadChildren"
    ],
    "scripts": {
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "e2e": "nx e2e demo-e2e",
        "sync-packages": "nx serve legacy-version-control --watch=false",
        "start": "nx serve",
        "format": "nx format:write --all .",
        "test:demo": "nx run-many --projects=demo,ngx-feature-flag-router --target=test && nx serve test-legacy --watch=false -c=source",
        "test:legacy": "nx serve test-legacy --watch=false",
        "test": "nx run-many --all --target=test --maxParallel=1 && npm run test:legacy",
        "build:demo": "nx build demo",
        "build:legacy": "nx serve build-legacy --watch=false",
        "build:lib": "nx run-many --all --exclude=build-legacy,test-legacy,legacy-utilities --target=build --maxParallel=1 && npm run build:legacy",
        "publish": "npm run sync-packages && npm run build:lib && npm run test && nx serve publish-legacy --watch=false",
        "deploy": "npm run test:demo && npm run build:demo && ng deploy --base-href=/ngx-feature-flag-router/ --cname=github.com"
    },
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,html}": [
            "eslint --ignore-path .gitignore --fix",
            "git add"
        ]
    },
    "dependencies": {
        "@angular/animations": "~13.2.0",
        "@angular/common": "~13.2.0",
        "@angular/compiler": "~13.2.0",
        "@angular/core": "~13.2.0",
        "@angular/forms": "~13.2.0",
        "@angular/platform-browser": "~13.2.0",
        "@angular/platform-browser-dynamic": "~13.2.0",
        "@angular/router": "~13.2.0",
        "@nrwl/angular": "13.9.3",
        "highlight.js": "^11.5.0",
        "rxjs": "~7.4.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.2.0",
        "@angular-eslint/eslint-plugin": "~13.0.1",
        "@angular-eslint/eslint-plugin-template": "~13.0.1",
        "@angular-eslint/template-parser": "~13.0.1",
        "@angular/cli": "~13.2.0",
        "@angular/compiler-cli": "~13.2.0",
        "@angular/language-service": "~13.2.0",
        "@nrwl/cli": "13.9.3",
        "@nrwl/cypress": "13.9.3",
        "@nrwl/eslint-plugin-nx": "13.9.3",
        "@nrwl/jest": "13.9.3",
        "@nrwl/linter": "13.9.3",
        "@nrwl/node": "^14.0.3",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/workspace": "13.9.3",
        "@types/edit-json-file": "^1.7.0",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "27.0.2",
        "@types/node": "16.11.7",
        "@typescript-eslint/eslint-plugin": "~5.10.0",
        "@typescript-eslint/parser": "~5.10.0",
        "angular-cli-ghpages": "^1.0.0",
        "cypress": "^9.1.0",
        "edit-json-file": "^1.7.0",
        "eslint": "~8.7.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "fs-extra": "^10.0.1",
        "husky": "^4.3.8",
        "jest": "27.2.3",
        "jest-preset-angular": "11.1.1",
        "lint-staged": "^12.3.7",
        "ng-packagr": "^13.0.0",
        "nx": "13.9.3",
        "postcss": "^8.4.5",
        "postcss-import": "^14.0.2",
        "postcss-preset-env": "^6.7.0",
        "postcss-url": "^10.1.1",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.5",
        "start-server-and-test": "^1.14.0",
        "ts-jest": "27.0.5",
        "ts-node": "9.1.1",
        "typescript": "~4.5.2"
    }
}
