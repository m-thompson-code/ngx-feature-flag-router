{
    "name": "ngx-feature-flag-router",
    "version": "14.0.5",
    "description": "Extends RouteModule functionality to allow for conditionally lazy-loading child routes based on feature flag. Allows for Services to configure Routes and includes helpers for preload strategy.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/m-thompson-code/ngx-feature-flag-router.git"
    },
    "bugs": {
        "url": "https://github.com/m-thompson-code/ngx-feature-flag-router/issues"
    },
    "homepage": "https://m-thompson-code.github.io/ngx-feature-flag-router/",
    "author": "Mark Thompson",
    "keywords": [
        "nx",
        "monorepo",
        "ng",
        "angular",
        "router",
        "routing",
        "RouterModule",
        "lazy loading",
        "dynamic",
        "conditional",
        "feature flag",
        "loadChildren",
        "rxjs",
        "A/B testing"
    ],
    "scripts": {
        "ng": "nx",
        "prepare": "husky install",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
        "format": "nx run-many --all --target=lint --fix && nx format:write --all .",
        "lint": "npm run format",
        "start": "nx serve demo",
        "e2e:demo": "nx run-many --projects=demo-e2e,legacy-e2e --target=e2e --maxParallel=1",
        "jest:demo": "nx run-many --projects=demo,ngx-feature-flag-router,legacy --target=test --maxParallel=1",
        "test:demo": "npm run jest:demo && npm run e2e:demo",
        "verify:demo": "npm run test:demo && nx build demo",
        "deploy:demo": "npm run test:demo && ng deploy --base-href=/ngx-feature-flag-router/ --cname=github.com",
        "sync-packages": "nx serve legacy-version-control",
        "jest:lib": "nx run-many --all --target=test --maxParallel=1",
        "e2e:lib": "nx serve legacy-test",
        "generate-jest-shield": "nx serve jest-coverage-shield",
        "test:lib": "npm run jest:lib && npm run e2e:lib && npm run generate-jest-shield",
        "build:lib": "nx serve legacy-build && nx serve schematics",
        "publish:lib": "npm run sync-packages && npm run test:lib && npm run build:lib && nx serve legacy-publish",
        "release": "npm run deploy:demo && npm run publish:lib -- --args=dry-run=false",
        "verify": "npm run verify:demo && npm run publish:lib -- --args=dry-run=true",
        "test": "npm run test:demo && npm run test:lib"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "14.0.2",
        "@angular/common": "14.0.2",
        "@angular/compiler": "14.0.2",
        "@angular/core": "14.0.2",
        "@angular/forms": "14.0.2",
        "@angular/platform-browser": "14.0.2",
        "@angular/platform-browser-dynamic": "14.0.2",
        "@angular/router": "14.0.2",
        "@nrwl/angular": "14.4.3",
        "highlight.js": "^11.5.0",
        "rxjs": "~7.4.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.0.2",
        "@angular-eslint/eslint-plugin": "14.0.2",
        "@angular-eslint/eslint-plugin-template": "14.0.2",
        "@angular-eslint/template-parser": "14.0.2",
        "@angular/cli": "~14.0.0",
        "@angular/compiler-cli": "14.0.2",
        "@angular/language-service": "14.0.2",
        "@bitovi/eslint-config": "^1.3.0",
        "@nrwl/cli": "14.4.3",
        "@nrwl/cypress": "14.4.3",
        "@nrwl/eslint-plugin-nx": "14.4.3",
        "@nrwl/jest": "14.4.3",
        "@nrwl/linter": "14.4.3",
        "@nrwl/node": "14.4.3",
        "@nrwl/nx-cloud": "14.2.0",
        "@nrwl/workspace": "14.4.3",
        "@types/fs-extra": "^9.0.13",
        "@types/glob": "^7.2.0",
        "@types/jest": "27.4.1",
        "@types/node": "18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "angular-cli-ghpages": "^1.0.0",
        "copyfiles": "^2.4.1",
        "cypress": "^9.1.0",
        "eslint": "~8.15.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "fs-extra": "^10.0.1",
        "glob": "^8.0.3",
        "husky": "^8.0.1",
        "jest": "27.5.1",
        "jest-preset-angular": "11.1.2",
        "lint-staged": "^12.3.7",
        "ng-packagr": "14.0.2",
        "nx": "14.4.3",
        "postcss": "^8.4.5",
        "postcss-import": "14.1.0",
        "postcss-preset-env": "7.5.0",
        "postcss-url": "10.1.3",
        "prettier": "2.7.1",
        "start-server-and-test": "^1.14.0",
        "ts-jest": "27.1.4",
        "ts-node": "10.8.1",
        "typescript": "~4.7.4"
    }
}
