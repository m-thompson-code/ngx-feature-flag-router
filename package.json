{
  "name": "ngx-feature-flag-router",
  "version": "13.0.2",
  "description": "Extends RouteModule functionality to allow for conditionally lazy-loading child routes based on feature flag. Allows for Services to configure Routes and includes helpers to preload strategies.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-thompson-code/ngx-feature-flag-router.git"
  },
  "bugs": {
    "url": "https://github.com/m-thompson-code/ngx-feature-flag-router/issues"
  },
  "homepage": "https://m-thompson-code.github.io/ngx-feature-flag-router/",
  "author": "Mark Thompson",
  "keywords": [
    "nx",
    "ng",
    "angular",
    "router",
    "RouterModule",
    "lazy loading",
    "conditional",
    "feature flag",
    "loadChildren"
  ],
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "format": "nx format:write --all .",
    "start": "nx serve demo",
    "e2e:demo": "nx run-many --projects=demo-e2e,legacy-e2e --target=e2e --maxParallel=1",
    "jest:demo": "nx run-many --projects=demo,ngx-feature-flag-router,legacy --target=test --maxParallel=1",
    "test:demo": "npm run jest:demo && npm run e2e:demo",
    "build:demo": "nx build demo",
    "deploy:demo": "npm run test:demo && npm run build:demo && ng deploy --base-href=/ngx-feature-flag-router/ --cname=github.com",
    "serve:build-legacy": "node dist/apps/build-legacy/main.js",
    "serve:test-legacy": "node dist/apps/test-legacy/main.js",
    "serve:publish-legacy": "node dist/apps/publish-legacy/main.js",
    "serve:legacy-version-control": "node dist/apps/legacy-version-control/main.js",
    "jest:lib": "nx run-many --all --target=test --maxParallel=1",
    "e2e:lib": "nx build test-legacy && npm run serve:test-legacy",
    "test:lib": "npm run jest:lib && npm run e2e:lib",
    "build:lib": "nx build build-legacy && npm run serve:build-legacy",
    "sync-packages": "nx build legacy-version-control && npm run serve:legacy-version-control",
    "publish:lib": "npm run sync-packages && npm run test:lib && npm run build:lib && nx build publish-legacy && npm run serve:publish-legacy"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "13.3.6",
    "@angular/common": "13.3.6",
    "@angular/compiler": "13.3.6",
    "@angular/core": "13.3.6",
    "@angular/forms": "13.3.6",
    "@angular/platform-browser": "13.3.6",
    "@angular/platform-browser-dynamic": "13.3.6",
    "@angular/router": "13.3.6",
    "@nrwl/angular": "14.1.9",
    "highlight.js": "^11.5.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.5",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.6",
    "@angular/language-service": "13.3.6",
    "@nrwl/cli": "14.1.9",
    "@nrwl/cypress": "14.1.9",
    "@nrwl/eslint-plugin-nx": "14.1.9",
    "@nrwl/jest": "14.1.9",
    "@nrwl/linter": "14.1.9",
    "@nrwl/node": "14.1.9",
    "@nrwl/nx-cloud": "14.1.2",
    "@nrwl/workspace": "14.1.9",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "angular-cli-ghpages": "^1.0.0",
    "cypress": "^9.1.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "fs-extra": "^10.0.1",
    "husky": "^4.3.8",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "lint-staged": "^12.3.7",
    "ng-packagr": "13.3.1",
    "nx": "14.1.9",
    "postcss": "^8.4.5",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.5.0",
    "postcss-url": "10.1.3",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "4.6.4"
  }
}
